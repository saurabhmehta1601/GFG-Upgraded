{
  "manifest_version": 3,
  "name": "GFG readtime",
  "version": "1.0.0",
  "description": "Get the readtime for articles on geeksforgeeks.org",
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/readtime.js"],
      "matches": ["https://www.geeksforgeeks.org/*"]
    }
  ]
}
